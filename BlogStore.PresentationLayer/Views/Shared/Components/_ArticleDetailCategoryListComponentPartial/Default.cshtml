@model List<BlogStore.DataAccessLayer.Dtos.GetCategoryWithArticleCountDto>
<div class="sidebar-box">
    <h3 class="heading">Kategoriler</h3>
    <ul class="categories">
        @foreach(var item in Model)
        {
        <li><a href="#">@item.CategoryName <span>(@item.ArticleCount)</span></a></li>
        }
    </ul>
</div>